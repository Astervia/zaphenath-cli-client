# Whether to skip the real on-chain call
ZAPHENATH_TEST_MOCK=true

# RPC URL to test against (e.g. Infura, Alchemy, local Ganache/Hardhat)
ZAPHENATH_TEST_RPC=http://localhost:8545

# Zaphenath deployed contract address
ZAPHENATH_TEST_CONTRACT=0x0000000000000000000000000000000000000001

# Path to a private key file (hex encoded, 0x prefix optional)
ZAPHENATH_TEST_PRIVKEY=./testkey.hex
